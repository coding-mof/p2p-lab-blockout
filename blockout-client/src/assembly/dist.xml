<assembly>
  <id>dist</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>target</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>conf/*</include>
        <include>start.bat</include>
        <include>start.sh</include>
        <include>background.jpg</include>
        <include>inventory_01.jpg</include>
        <include>*.jar</include>
        <include>natives/*</include>
        <include>maps/*</include>
        <include>*.properties</include>
      </includes>
    </fileSet>
  </fileSets>
  <dependencySets>
    <dependencySet>
      <outputDirectory>/lib</outputDirectory>
      <unpack>false</unpack>
      <scope>runtime</scope>
    </dependencySet>
  </dependencySets>
</assembly>
